@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="col-md-6 mx-auto">
    <div class="card">
        <div class="card-header">
            <h4>Ingrese un Numero de Liquidacion </h4>
        </div>
        <div class="card-body">
            <form action="/Depositos/ImprimirPDF" method="post">
                <div class="form-group">
                    <input type="text" name="id" class="form-control" placeholder="No Liquidacion" autofocus />
                </div>

                <button class="btn btn-primary" type="submit">
                    Buscar
                </button>
                <button class="btn btn-primary" type="submit">
                    imprimir
                </button>
            </form>
        </div>
    </div>
</div>

